{
  "name": "vite-plus-template",
  "private": true,
  "version": "0.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "lint": "eslint .",
    "fix": "eslint --fix . && prettier --write .",
    "preview": "vite preview",
    "test": "vitest"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.8",
    "lucide-react": "^0.475.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "prettier": "^3.5.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.6"
  },
  "imports": {
    "#src/*": [
      "./src/*"
    ],
    "#components/*": [
      "./src/components/*"
    ],
    "#assets/*": [
      "./src/assets/*"
    ],
    "#pages/*": [
      "./src/pages/*"
    ],
    "#hooks/*": [
      "./src/hooks/*"
    ],
    "#context/*": [
      "./src/context/*"
    ],
    "#utils/*": [
      "./src/utils/*"
    ],
    "#data/*": [
      "./src/data/*"
    ],
    "#lib/*": [
      "./src/libs/*"
    ]
  },
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ],
    "semi": false,
    "jsxSingleQuote": false
  }
}
